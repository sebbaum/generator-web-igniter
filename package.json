{
  "name": "generator-web-igniter",
  "version": "0.3.1",
  "description": "A yeoman generator to kickstart your frontend development.",
  "homepage": "https://github.com/sebbaum/generator-web-igniter",
  "author": {
    "name": "Sebastian Baum",
    "email": "seb.baum@googlemail.com",
    "url": "http://www.sebbaum.de"
  },
  "contributors": [
    {
      "name": "Sebastian Baum",
      "email": "seb.baum@googlemail.com",
      "url": "http://www.sebbaum.de"
    },
    {
      "name": "Markek",
      "email": "marek.jurek.gebka.@gmail.com"
    }
  ],
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "webapp",
    "frontend"
  ],
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "sebbaum/generator-web-igniter",
  "license": "Apache-2.0"
}
